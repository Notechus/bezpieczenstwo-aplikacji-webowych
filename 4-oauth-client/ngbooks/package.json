{
  "name": "ngbooks",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "cross-env ng serve --poll=2000",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=8192 ng build ngBooks",
    "build:prod": "cross-env NODE_OPTIONS=--max_old_space_size=8192 ng build ngBooks --prod --sourceMap=false --progress=false",
    "build:prod:sourcemap": "cross-env NODE_OPTIONS=--max_old_space_size=8192 ng build ngBooks --prod --sourceMap=true --progress=false",
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "testcoverage": "jest --coverage",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
